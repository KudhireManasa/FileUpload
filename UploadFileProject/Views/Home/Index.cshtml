@{

}
@model List<string>
<div class="container">
    <div class="row">
        @foreach (var images in Model)
        {
            <div class="col-md-3">
                <div class="alert alert-success" role="alert">
                    <img src="~/@images" alt="Alternate Text" height="100px" width="205px" />
                </div>
            </div>

        }
    </div>

    <div class="container">
        @using (Html.BeginForm("Index", "Gallary", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <div class="row">
                <div class="form-group col-md-6">
                    <input type="file" class="form-control" name="myFile" />
                </div>
                <div class="form-group col-md-6">
                    <input type="submit" class="filesubmit" name="upload" value="upload" />
                </div>
            </div>

        }
    </div>
</div>